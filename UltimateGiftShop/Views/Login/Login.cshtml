@model LoginUser
@{
    ViewData["Title"] = "Login";
}

<div class="text-center">
    @* <h1 class="display-4">@Model.LoggedIn ? You are logged In : Unsuccessful Login</h1> *@
@if (Model.LoggedIn)
            {
               <h1 class="display-4">Logged In</h1>
            }
            else
            {
                <h1 class="display-4">Unsuccessfull Log In</h1>
            }
</div>

