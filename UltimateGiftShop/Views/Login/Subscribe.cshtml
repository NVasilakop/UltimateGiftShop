@model AppDataModels.Models.SubscribeUser

@using (Html.BeginForm("Subscribe", "Login", FormMethod.Post))
{
    <table>
        <tr>
            <td>Enter ID: </td>
            <td>@Html.TextBoxFor(m => m.UserName)</td>
            <td>Enter Password</td>
            <td>@Html.TextBoxFor(m => m.Password)</td>
            <td>
                <input type="submit" value="Subscribe"/>
            </td>
        </tr>
    </table>
}

